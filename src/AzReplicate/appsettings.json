{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http": "None"
    },

    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System.Net.Http": "None"
      },
      "EnableAdaptiveSampling": false,
      "FilteredDependencyTypes": [
        "Azure queue",
        "Azure blob",
        "Azure table",
        "Http",
        "Https"
      ]
    }
  }
}